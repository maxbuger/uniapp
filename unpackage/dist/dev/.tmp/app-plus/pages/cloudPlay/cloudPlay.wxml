<view class="wrap" bind:__l="__l"><wuc-tab tab-list="{{tabList3}}" textFlex tabCur="{{tabCur}}" tab-class="text-center bg-white tab-wrap" select-class="text-orange" data-event-opts="{{[['^updateTabCur',[['__set_sync',['$0','tabCur','$event'],['']]]],['^updateTabCur',[['__set_sync',['$0','tabCur','$event'],['']]]]]}}" bind:updateTabCur="__e"></wuc-tab><swiper class="swiper" current="{{tabCur}}" duration="500" circular="{{true}}" indicator-color="rgba(255,255,255,0)" indicator-active-color="rgba(255,255,255,0)" data-event-opts="{{[['change',[['swiperChange3',['$event']]]]]}}" bindchange="__e"><swiper-item class="swiper-item"><block wx:for="{{novelList}}" wx:for-item="nove" wx:for-index="noveIndex" wx:key="noveIndex"><view data-event-opts="{{[['tap',[['getCateId',[0,'$0','$1'],[[['novelList','',noveIndex,'id']],[['novelList','',noveIndex,'title']]]]]]]}}" class="bg-white text-center text-black view-content" bindtap="__e"><view class="image"><image src="{{nove.image}}" alt></image></view><view class="title">{{nove.title}}</view></view></block></swiper-item><swiper-item class="swiper-item"><block wx:for="{{vipCateList}}" wx:for-item="vip" wx:for-index="vipIndex" wx:key="vipIndex"><view data-event-opts="{{[['tap',[['openVipLink',['$0'],[[['vipCateList','',vipIndex,'link']]]]]]]}}" class="bg-white text-center text-black view-content" bindtap="__e"><view class="image"><image src="{{vip.image}}" alt></image></view><view class="title">{{vip.title}}</view></view></block></swiper-item><swiper-item class="swiper-item"><block wx:for="{{videoList}}" wx:for-item="video" wx:for-index="videoIndex" wx:key="videoIndex"><view data-event-opts="{{[['tap',[['getCateId',[2,'$0','$1'],[[['videoList','',videoIndex,'id']],[['videoList','',videoIndex,'title']]]]]]]}}" class="bg-white text-center text-black view-content" bindtap="__e"><view class="image"><image src="{{video.image}}" alt></image></view><view class="title">{{video.title}}</view></view></block></swiper-item></swiper></view>