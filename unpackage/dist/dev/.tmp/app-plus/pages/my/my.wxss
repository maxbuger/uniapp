@font-face {
	font-family: myIcons;
	font-weight: normal;
	font-style: normal;
	src: url('https://at.alicdn.com/t/font_1223415_8ubgd8zycnt.eot');
	src: url('https://at.alicdn.com/t/font_1223415_8ubgd8zycnt.eot?#iefix') format('embedded-opentype'),
		url('https://at.alicdn.com/t/font_1223415_8ubgd8zycnt.woff2') format('woff2'),
		url('https://at.alicdn.com/t/font_1223415_8ubgd8zycnt.woff') format('woff'),
		url('https://at.alicdn.com/t/font_1223415_8ubgd8zycnt.ttf') format('truetype'),
		url('https://at.alicdn.com/t/font_1223415_8ubgd8zycnt.svg#iconfont') format('svg');
}
.my-icon {
	width: 40rpx;
	height: 90rpx;
	line-height: 90rpx;
	font-size: 34rpx;
	color: #B13531;
	text-align: center;
	font-family: myIcons;
	margin-right: 20rpx;
}


page,
view {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
page {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	min-height: 100%;
	background-color: #EFEFEF;
}
template {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}
.vip-tip {
	height: 90rpx;
	line-height: 90rpx;
	font-size: 34rpx;
	color: #B13531;
	text-align: right;
	font-family: texticons;
}

